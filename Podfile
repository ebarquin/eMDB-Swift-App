# platform :ios, '10.0'

target 'EMDb' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for EMDb
  pod 'Alamofire'
  pod 'Kingfisher'
  pod 'SwiftyJSON', :git => 'https://github.com/appsailor/SwiftyJSON', :branch => 'swift3' 

end
